<h1><a href="/">Links</a></h1>
<ul id="links">
  <% @links.each do |link| %>
    <li>
      Title: <a href="<%= link.url %>" target="_blank"><%= link.title %></a>
      <br>
      URL: <%= link.url %>
      <br>
      Tags: <% link.tags.each do |tag| %>
            <%= tag.name %>
            <% end %>
    </li>
  <% end %>
</ul>
